{
    "name": "idci/request-theme-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle that easily allow to associate theme with a HTTP Request",
    "keywords": ["http request","theme","twig","customize"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Bondaz",
            "email": "gabriel.bondaz@idci-consulting.fr",
            "homepage": "http://www.idci-consulting.fr"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "symfony/symfony": "~2.3",
        "twig/twig": "*",
        "twig/extensions": "~1.0",
        "doctrine/inflector": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "IDCI\\Bundle\\RequestThemeBundle": "" }
    },
    "target-dir": "IDCI/Bundle/RequestThemeBundle",
    "minimum-stability": "dev"
}